{
  "name": "type-converter",
  "private": false,
  "version": "0.0.5",
  "description": "A type conversion library for TypeScript (using decorators)",
  "repository": {
    "type": "git",
    "url": "https://github.com/techfeed/type-converter.git"
  },
  "bugs": {
    "url": "https://github.com/techfeed/type-converter/issues"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "mocha --recursive --require ts-node/register \"src/**/*.spec.ts\"",
    "cover": "nyc npm test",
    "clean": "rm -rf dist",
    "build-publish": "npm run build && cp package.json dist && npm publish dist"
  },
  "author": {
    "name": "Shumpei Shiraishi",
    "email": "s@techfeed.co.jp"
  },
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "sourceMap": true,
    "all": true
  }
}
